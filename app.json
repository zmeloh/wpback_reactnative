{
  "expo": {
    "name": "spiapp",
    "slug": "spiapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-spi.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET"
      ],
      "package": "com.zmeloh.spiapp"
    },
    "web": {
      "favicon": "./assets/logo-spi.png"
    },
    "extra": {
      "eas": {
        "projectId": "fcf0a86a-0d30-4bd6-ab4c-11a65e47aab8"
      }
    }
  }
}
